@model IEnumerable<alert_roster.web.Models.Message>

@{
    ViewBag.Title = "Messages";
}

<div class="jumbotron">
    <h1>@ViewBag.Title</h1>
</div>

@foreach (var message in Model)
{
    <div class="row">
        <div class="col-md-4">
            <fieldset>
                <legend>
                    <script>var d = moment.utc('@message.PostedDate').local(); document.write(d);</script>
                </legend>

                @message.Content
            </fieldset>
        </div>
    </div>
}